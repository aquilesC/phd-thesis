arxivId = {cond-mat/0008204},
author = {Hirsch, Leon R and Stafford, R J and Bankson, J a and Sershen, S R and Rivera, B and Price, R E and Hazle, J D and Halas, Naomi J and West, J L},
doi = {10.1073/pnas.2232479100},
eprint = {0008204},
file = {:C$\backslash$:/Users/aquiles/Papers/Hirsch et al/Hirsch et al. - 2003 - Nanoshell-mediated near-infrared thermal therapy of tumors under magnetic resonance guidance.pdf:pdf},
isbn = {0027-8424},
issn = {0027-8424},
journal = {Proc. Natl. Acad. Sci. U. S. A.},
keywords = {Animals,Cell Line, Tumor,Female,Gold,Gold: chemistry,Humans,Hyperthermia, Induced,Infrared Rays,Magnetic Resonance Imaging,Magnetic Resonance Spectroscopy,Magnetic Resonance Spectroscopy: methods,Mice,Mice, SCID,Models, Statistical,Nanotechnology,Neoplasms,Neoplasms: therapy,Silicon,Silicon: chemistry,Temperature},
