journal = {Small},
keywords = {Biocompatibility,Cytoxicity,Nanomaterials,Nanoparticles,Toxicology},
number = {1},
pages = {26--49},
pmid = {18165959},
title = {{Cytotoxicity of nanopartides}},
volume = {4},
year = {2008}
}
