keywords = {and carrier mobility,attice strain produced by,bending deformation can,energies,material properties,providing the,signi fi cantly alter,spectroscopy,strained nanomaterials,such as band,symmetry,ultrafast imaging},
month = {sep},
pmid = {25259929},
title = {{Reversible Strain-Induced Electron-Hole Recombination in Silicon Nanowires Observed with Femtosecond Pump-Probe Microscopy.}},
url = {http://pubs.acs.org/doi/abs/10.1021/nl5026166 http://www.ncbi.nlm.nih.gov/pubmed/25259929},
year = {2014}
}
