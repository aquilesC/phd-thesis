month = {oct},
number = {10},
pages = {4058--66},
pmid = {21902194},
title = {{Plasmon-driven selective deposition of au bipyramidal nanoparticles.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/21902194},
volume = {11},
year = {2011}
}
