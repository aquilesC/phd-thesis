doi = {10.1021/nn5055283},
issn = {1936-0851},
journal = {ACS Nano},
keywords = {gold nanorods,photothermal reshaping,plasmonics,surface diffusion},
month = {dec},
number = {12},
pages = {12071--12079},
pmid = {25405517},
title = {{Below Melting Point Photothermal Reshaping of Single Gold Nanorods Driven by Surface Diffusion}},
