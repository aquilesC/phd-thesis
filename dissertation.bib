file = {:C$\backslash$:/Users/aquiles/Papers/Finnigan et al/Finnigan et al. - 2012 - Evolution of increased complexity in a molecular machine.pdf:pdf;:C$\backslash$:/Users/aquiles/Papers/Finnigan et al/Finnigan et al. - 2012 - Evolution of increased complexity in a molecular machine(2).pdf:pdf},
issn = {1476-4687},
journal = {Nature},
