keywords = {au nanowires,cally synthesized noble metal,chemi-,he unique size and,morphology de-,nano-,nanophotonics,optical manipulation,optical tweezers,pendent optical properties of,plasmonics},
month = {oct},
number = {10},
pages = {8794--800},
pmid = {24041038},
title = {{Why single-beam optical tweezers trap gold nanowires in three dimensions.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/24041038},
volume = {7},
