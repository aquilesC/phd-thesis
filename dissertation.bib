title = {{Analyzing protein structure and function using ancestral gene reconstruction.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2916957{\&}tool=pmcentrez{\&}rendertype=abstract},
volume = {20},
year = {2010}
}
