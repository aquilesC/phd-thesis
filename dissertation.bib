pmid = {15159019},
title = {{Photo-thermal tumor ablation in mice using near infrared-absorbing nanoparticles}},
url = {http://linkinghub.elsevier.com/retrieve/pii/S0304383504001442},
volume = {209},
year = {2004}
}
