@article{Pissuwan2006,
abstract = {Nanoparticles of gold, which are in the size range 10-100 nm, undergo a plasmon resonance with light. This is a process whereby the electrons of the gold resonate in response to incoming radiation causing them to both absorb and scatter light. This effect can be harnessed to either destroy tissue by local heating or release payload molecules of therapeutic importance. Gold nanoparticles can also be conjugated to biologically active moieties, providing possibilities for targeting to particular tissues. Here, we review the progress made in the exploitation of the plasmon resonance of gold nanoparticles in photo-thermal therapeutic medicine. ?? 2005 Elsevier Ltd. All rights reserved.},
