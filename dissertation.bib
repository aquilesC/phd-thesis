author = {Donehue, Jessica E. and Wertz, Esther and Talicska, Courtney N. and Biteen, Julie S.},
doi = {10.1021/jp504186n},
file = {:C$\backslash$:/Users/aquiles/Papers/Donehue et al/Donehue et al. - 2014 - Plasmon-Enhanced brightness and photostability from single fluorescent proteins coupled to gold nanorods.pdf:pdf},
issn = {19327455},
